<!DOCTYPE html>
<html>
<head>
  <title><%= APP_CONFIG['site']['title']%></title>
  <%= stylesheet_link_tag    "dashboard/bootstrap.min.css", "dashboard/font-awesome.min.css","dashboard/ui-lightness/jquery-ui-1.10.0.custom.min.css","dashboard/base-admin-2.css", "/dashboard/base-admin-2-responsive.css", "dashboard/pages/dashboard.css", "dashboard/custom.css", "dashboard", :media => "all" %>

    <link href="http://fonts.googleapis.com/css?family=Open+Sans:400italic,600italic,400,600" rel="stylesheet" />

  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
	<%= render "layouts/menu_dashboard" %>



    
<div class="subnavbar">

  <div class="subnavbar-inner">
  
    <div class="container">
      
      <a class="btn-subnavbar collapsed" data-toggle="collapse" data-target=".subnav-collapse">
        <i class="icon-reorder"></i>
      </a>

      <div class="subnav-collapse">
        <ul class="mainnav">
        
          <li class="active">
            <%= link_to dashboard_path do%>
              <i class="icon-home"></i>
              <span>Home</span>
            <%end%>  
          </li>
          
          <li>         
            <%= link_to postnotices_path do%>
              <i class="icon-copy"></i>
              <span>Not√≠cias</span>
            <%end%>  
          </li>
          
          <li> 
            <%= link_to cities_path do%>
               <i class="icon-building"></i>
              <span>Cidades</span>
            <%end%>           
          </li>
          
          <li>  
            <%= link_to categories_path do%>
              <i class="icon-th-list"></i>
              <span>Categorias</span>
            <%end%>          
          </li>
        
        </ul>
      </div> <!-- /.subnav-collapse -->

    </div> <!-- /container -->
  
  </div> <!-- /subnavbar-inner -->

</div> <!-- /subnavbar -->


      <%= yield %>







<div id="fb-root"></div>
<script>(function(d, s, id) {
  var js, fjs = d.getElementsByTagName(s)[0];
  if (d.getElementById(id)) return;
  js = d.createElement(s); js.id = id;
  js.src = "//connect.facebook.net/pt_BR/all.js#xfbml=1";
  fjs.parentNode.insertBefore(js, fjs);
}(document, 'script', 'facebook-jssdk'));</script>
<style type="text/css">
	
#wrap {
    height: auto !important;
    margin: 0 auto -60px;
    min-height: 100%;
}

#footer {
    background-color: #F5F5F5;
}
#push, #footer {
    height: 60px;
}

html, body {
    height: 100%;
}

</style>


<script src="assets/dashboard/libs/jquery-1.8.3.min.js"></script>
<script src="assets/dashboard/libs/jquery-ui-1.10.0.custom.min.js"></script>
<script src="assets/dashboard/libs/bootstrap.min.js"></script>

<script src="assets/dashboard/plugins/flot/jquery.flot.js"></script>
<script src="assets/dashboard/plugins/flot/jquery.flot.pie.js"></script>
<script src="assets/dashboard/plugins/flot/jquery.flot.resize.js"></script>

<script src="assets/dashboard/Application.js"></script>

<script src="assets/dashboard/charts/area.js"></script>
<script src="assets/dashboard/charts/donut.js"></script>
</body>
</html>
