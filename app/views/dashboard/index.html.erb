          <ul class="news-items">
        
              

                <% @postnotices.reverse.take(5).each do |post| %>
                <li>
                  <div class="news-item-detail"> 
                    <%= link_to post.title, post, :class=>"news-item-title"%>
                    <p class="news-item-preview">
                      <%= post.descbanner %>
                       
                    </p>  
                  </div>

                  <div class="news-item-date">

                    <span class="news-item-day">
                    	<%= post.startdate.to_s[8..10] %>
                    </span>
                    <span class="news-item-month">
                    	<%date =  post.startdate.to_s[5..6] %>

                    	<%= case date
                    	when "01"
                    		"Jan"
                    	when "02"
                    		"Fev"
                    	when "03"
                    		"Mar"
                    	when "04" 
                    		"Abr"
                    	when "05"
                    		"Mai"
                    	when "06"
                    		"Jun"
                    	when "07"
                    		"Jul"
                    	when "08"
                    		"Ago"
                    	when "09"
                    		"Set"
                    	when "10"
                    		"Out"
                    	when "11"
                    		"Nov"
                    	when "12"
                    		"Dez"
                    	end%>
                    </span>
                  </div>  
                  </li>
                <%end%>

     
          </ul>
          
